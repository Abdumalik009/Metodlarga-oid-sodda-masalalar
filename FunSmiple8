package FunSimple;
import java.util.Scanner;

public class FunSimple8 {

        public static void main(String[] args) {
            Scanner scanner = new Scanner(System.in);

            System.out.print("K ni kiriting: ");
            int k = scanner.nextInt();

            System.out.print("R raqamini kiriting (1 <= R <= 9): ");
            int r = scanner.nextInt();

            int result = addRightDigit(k, r);
            System.out.println("Natija: " + result);

            scanner.close();
        }

        public static int addRightDigit(int number, int digit) {
            return number * 10 + digit;
        }
    }


